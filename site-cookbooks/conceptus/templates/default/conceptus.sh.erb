#!/bin/sh

APPHOME=<%= @appdir %>

exec java -Djava.security.egd=file:/dev/urandom -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=1044 -jar "$APPHOME/conceptus-1.1.9.jar" --spring.config.location="file://$APPHOME/application.properties,file://$APPHOME/application.yaml" >>"$APPHOME/log/conceptus.log" 2>&1
